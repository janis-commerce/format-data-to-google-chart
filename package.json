{
  "name": "@janiscommerce/format-data-to-google-chart",
  "version": "1.0.0",
  "description": "A package that formats the data for use in Google charts",
  "main": "lib/index.js",
  "scripts": {
    "test": "export TEST_ENV=true; mocha --require tests/bootstrap.js --exit -R nyan --recursive tests/unit/",
    "watch-test": "export TEST_ENV=true; mocha --exit -R nyan -w --recursive tests/",
    "test-ci": "nyc --reporter=lcov --reporter=text-summary mocha --exit --recursive tests/",
    "coverage": "nyc npm test",
    "lint": "eslint lib/ tests/"
  },
  "files": [
    "/lib"
  ],
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/janis-commerce/format-data-to-google-chart.git"
  },
  "keywords": [
    "list",
    "chart",
    "google",
    "charts",
    "graphs"
  ],
  "author": "Janis",
  "license": "ISC",
  "homepage": "https://github.com/janis-commerce/format-data-to-google-chart.git#readme",
  "devDependencies": {
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "husky": "^4.3.0",
    "mocha": "^8.1.3",
    "nyc": "^15.0.1",
    "sinon": "^9.0.2"
  },
  "dependencies": {
    "lllog": "^1.1.2"
  }
}
